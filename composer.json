{
  "name": "php-recipes/injection",
  "description": "A supporting package for dealing with injection in PHP. This packages includes a series of interfaces, traits, attributes and helpers for dealing with all forms of dependency injection in PHP.",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Ollie Read",
      "email": "code@ollie.codes"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "sempro/phpunit-pretty-print": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "PhpR\\Injection\\": "src/"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
    },
    "files": [
    ]
  },
  "scripts": {
    "phpunit": "phpunit --verbose --colors=always",
    "phpunit-coverage": "XDEBUG_MODE=coverage phpunit --verbose --colors=always --coverage-html build/coverage",
    "test": [
      "@phpunit"
    ]
  }
}